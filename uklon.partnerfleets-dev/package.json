{
  "name": "partnerfleets",
  "version": "1.80.1",
  "private": true,
  "engines": {
    "node": "^20.11.1",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "start": "nx run-many --all --parallel --target=serve",
    "start:ui": "nx run ui:serve",
    "start:api": "nx run api:serve:development",
    "start:api:staging": "nx run api:serve:staging",
    "start:mock": "cross-env MOCK=true nx run-many --all --parallel --target=serve",
    "start:e2e:staging": "cross-env DEV=true concurrently npm:start:api:staging npm:e2e:watch",
    "start:e2e:mobile:staging": "cross-env DEV=true concurrently npm:start:api:staging npm:e2e:watch:mobile",
    "build": "nx run-many --target=build --projects=api,ui --configuration production",
    "test": "nx run-many --all --parallel=1 --target=test --runInBand",
    "lint": "nx run-many --all --parallel=1 --target=lint",
    "lint:fix": "nx run-many --all --parallel=1 --target=lint --fix --skip-nx-cache",
    "slint": "stylelint \"./apps/ui/src/**/*.scss\"",
    "slint:fix": "stylelint \"./apps/ui/src/**/*.scss\" --fix",
    "e2e": "cross-env TZ=UTC nx e2e ui-e2e --skip-nx-cache",
    "e2e:watch": "cross-env TZ=UTC nx e2e ui-e2e --open --browser chrome --watch --skip-nx-cache",
    "e2e:watch:mock": "cross-env TZ=UTC UseMockData=true nx e2e ui-e2e --open --browser chrome --watch --skip-nx-cache",
    "e2e:mobile": "cross-env TZ=UTC nx e2e-mobile ui-e2e --skip-nx-cache",
    "e2e:watch:mobile": "cross-env TZ=UTC nx e2e-mobile ui-e2e --open --browser chrome --watch --skip-nx-cache",
    "bundle:analyzer": "nx run build --project=ui --configuration production --stats-json && webpack-bundle-analyzer dist/apps/ui/stats.json"
  },
